<link href="/assets/gsp/green/gsp.green.review.css" media="all" rel="stylesheet" type="text/css" />

<div class="panel" id="completed-task-panel">
  <h2>Your Completed Tasks</h2>
  <div>
    <table id="completed_tasks_list">
      <% if @recently_completed_tasks.empty? %>
      <tr>
        <td colspan="3" align="center">No completed tasks</td>
      </tr>
      <% else %>
      <tr>
        <th>&nbsp;</th>
        <th>&nbsp;</th>
        <th>Task</th>
        <th>Name</th>
        <th>Task Start Date</th>
        <th>Task Due</th>
      </tr>
      <% @recently_completed_tasks.each do |task| %>
      <%= render 'review/recently_completed_tasks_row', :task => task %>
      <% end %>
      <% end %>
    </table>
  </div>
</div>

